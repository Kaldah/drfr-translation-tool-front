{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        "http://localhost:1420",
        "http://localhost:3000"
      ]
    },
    "shell:default",
    "shell:default",
    "store:default"
  ]
}